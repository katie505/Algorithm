    "N = int(input())\n",
    "nums = map(int, input().split())\n",
    "\n",
    "count = 0\n",
    "for num in nums:\n",
    "    error = 0\n",
    "    if num > 1:\n",
    "        for i in range(2, num):\n",
    "            if num % i == 0:\n",
    "                error += 1\n",
    "        if error == 0:\n",
    "            count += 1\n",
    "\n",
    "print(count)"
   ]
