    "sugar = int(input())\n",
    "\n",
    "bag = 0\n",
    "\n",
    "while sugar >= 0:\n",
    "    if sugar % 5 == 0:\n",
    "        bag += sugar // 5\n",
    "        print(bag)\n",
    "        break\n",
    "    sugar -= 3\n",
    "    bag += 1\n",
    "else:\n",
    "    print(-1)"
